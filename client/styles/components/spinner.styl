@-webkit-keyframes spinningBounce
  0%, 100%
    -webkit-transform: scale(0.0)
  50%
    -webkit-transform: scale(1.0)


@keyframes spinningBounce
  0%, 100%
    transform: scale(0.0)
    -webkit-transform: scale(0.0)
  50%
    transform: scale(1.0)
    -webkit-transform: scale(1.0)

.spinner
  width: 50px
  height: 50px
  position: fixed
  top: 50%
  left: 50%
  z-index: 10
  margin-top: -25px
  margin-left: -25px
  opacity: 0
  transition: opacity 1s
  .spinner-ie
    display: none

.double-bounce1, .double-bounce2
  width: 100%
  height: 100%
  border-radius: 50%
  background-color: Red
  opacity: 0.6
  position: absolute
  top: 0
  left: 0
  animation: spinningBounce 2.0s infinite ease-in-out

.double-bounce2
  -webkit-animation-delay: -1.0s
  animation-delay: -1.0s
